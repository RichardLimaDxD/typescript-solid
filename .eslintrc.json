{
  "parser": "@typescript-eslint/parser",
  "extends": ["@rocketseat/eslint-config/node"],
  "rules": {
    "camelcase": "off",
    "no-useless-constructor": "off",
    "semi": ["error", "always"],
    "prettier/prettier": [
      "error",
      {
        "semi": true,
        "singleQuote": false
      }
    ]
  }
}
